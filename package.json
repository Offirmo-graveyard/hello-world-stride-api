{
  "name": "hello-world-stride-api",
  "version": "0.0.1",
  "description": " A hello world for Stride API ",
  "main": "index.js",
  "repository": "https://github.com/Offirmo/hello-world-stride-api.git",
  "author": "Offirmo <offirmo.net@gmail.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": "7.x"
  },
  "scripts": {
    "__prod": "~~~~~~ PRODUCTION ~~~~~~",
    "clean": "rm -rf dist",
    "transpile": "tsc --project meta/prod",
    "copy-other-files": "cpx 'src/**/*.{dust,css,png,json}' dist/src.es7.cjs",
    "build": "npm-run-all clean --parallel transpile copy-other-files",
    "start": "node dist/src.es7.cjs/index.js",
    "__dev": "~~~~~~ DEVELOPMENT ~~~~~~",
    "dev1": "tsc -p meta/dev --watch",
    "dev2": "npm run copy-other-files -- --watch --verbose",
    "dev": "npm-run-all clean --parallel dev1 dev2",
    "start:dev": "nodemon --delay 1 --config meta/dev/nodemon.json dist/src.es7.cjs/index.js | ./node_modules/.bin/bunyan",
    "test": "mocha --opts node_modules/@offirmo/unit-test-toolbox/mocha.opts node_modules/@offirmo/unit-test-toolbox/mocha-chai-init-node.js 'dist/src.es7.cjs/**/*spec.js'",
    "__misc": "~~~~~~ MISC ~~~~~~",
    "__local_tools": "~~~~~~ Tools installed locally ~~~~~~",
    "__shortcuts": "~~~~~~ Shortcuts ~~~~~~"
  },
  "dependencies": {
    "@offirmo/loggers-types-and-stubs": "^0.0.3",
    "body-parser": "^1.17.1",
    "boxen": "^1.2.1",
    "bunyan": "^1.8.10",
    "connect-redis": "^3.2.0",
    "consolidate": "^0.14.5",
    "dustjs-helpers": "^1.7.3",
    "dustjs-linkedin": "^2.7.5",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "helmet": "^3.5.0",
    "jwt-simple": "^0.5.1",
    "linewrap": "^0.2.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.26",
    "morgan": "^1.8.1",
    "prettyjson": "^1.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "request": "^2.83.0",
    "simplyconfig": "^0.4.0",
    "typescript-string-enums": "^0.3.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@offirmo/unit-test-toolbox": "^2.1.0",
    "@types/body-parser": "^1.16.3",
    "@types/bunyan": "^1.8.3",
    "@types/connect-redis": "^0.0.6",
    "@types/consolidate": "^0.0.33",
    "@types/dustjs-linkedin": "^1.2.30",
    "@types/express": "^4.0.35",
    "@types/express-session": "^1.15.3",
    "@types/helmet": "^0.0.36",
    "@types/lodash": "^4.14.63",
    "@types/mongodb": "^2.1.43",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8",
    "@types/react-dom": "^15.5.0",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^3.4.2",
    "cpx": "^1.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "pre-commit": "^1.2.2",
    "supertest": "^3.0.0",
    "typescript": "^2.2.2"
  },
  "pre-commit": [
    "build"
  ]
}
