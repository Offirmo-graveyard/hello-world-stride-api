{
	"baseUrl": "https://7282ab55.ngrok.io/demo",
	"key": "demo-app",
	"lifecycle": {
		"installed": "/installed",
		"uninstalled": "/uninstalled"
	},
	"modules": {
		"chat:bot": [
			{
				"key": "demo-bot",
				"mention": {
					"url": "/bot-mentioned"
				},
				"directMessage": {
					"url": "/bot-directly-messaged"
				}
			}
		],
		"chat:sidebar": [
			{
				"key": "demo-app-sidebar",
				"name": {
					"value": "App Sidebar"
				},
				"url": "/sidebar.html",
				"authentication": "jwt"
			}
		],
		"chat:glance": [
			{
				"key": "demo-app-glance",
				"name": {
					"value": "App Glance"
				},
				"icon": {
					"url": "/logo.png",
					"url@2x": "/logo.png"
				},
				"target": "demo-app-sidebar",
				"queryUrl": "/glance-state",
				"authentication": "jwt"
			}
		]
	}
}
